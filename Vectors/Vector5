module top_module (
    input a, b, c, d, e,
    output [24:0] out );//
    
    logic [24:0] v1, v2;
    
    assign v1 = {{5{a}}, {5{b}}, {5{c}}, {5{d}}, {5{e}}};
    assign v2 = {5{{a, b, c, d, e}}};
    
    assign out = ~v1 ^ v2;

    // The output is XNOR of two vectors created by 
    // concatenating and replicating the five inputs.
    // assign out = ~{ ... } ^ { ... };

endmodule
